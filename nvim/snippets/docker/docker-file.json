{
  "FROM": {
    "prefix": "from",
    "body": "FROM ${1:ubuntu}"
  },
  "LABEL": {
    "prefix": "lab",
    "body": "LABEL ${1:label}=\"${2:value}\""
  },
  "Label maintainer": {
    "prefix": "labm",
    "body": "LABEL maintainer=\"${1:name}\""
  },
  "RUN": {
    "prefix": "run",
    "body": ["RUN ${1:command}"]
  },
  "CMD": {
    "prefix": "cmd",
    "body": ["CMD ${1:command}"]
  },
  "COPY": {
    "prefix": "copy",
    "body": ["COPY ${1:src} ${2:dest}"]
  },
  "EXPOSE": {
    "prefix": "expose",
    "body": ["EXPOSE ${1:port}"]
  },
  "ENV": {
    "prefix": "env",
    "body": ["ENV ${1:key} ${2: value}"]
  },
  "ADD": {
    "prefix": "add",
    "body": ["ADD ${1:src} ${2:dst}"]
  },
  "ENTRYPOINT": {
    "prefix": "entry",
    "body": "ENTRYPOINT ${1:command}"
  },
  "VOLUME": {
    "prefix": "vol",
    "body": "VOLUME [\"${1:path}\"]"
  },
  "USER": {
    "prefix": "user",
    "body": "USER ${1:name}"
  },
  "WORKDIR": {
    "prefix": "work",
    "body": "WORKDIR ${1:name}"
  },
  "Update Packages": {
    "prefix": "upd",
    "body": [
      "RUN echo \"deb http://archive.ubuntu.com/ubuntu ${1:precise} main universe\" > /etc/apt/sources.list; \\",
      "apt-get update && apt-get -y upgrade; \\ ",
      "${2}; \\",
      "rm -rf /var/lib/apt/lists/*"
    ]
  },
  "HEAD": {
    "prefix": "head",
    "body": ["# ${1:description}", "#  ", "# VERSION ${2:0.1.0}", "${3}"]
  },
  "ONBUILD": {
    "prefix": "onb",
    "body": "ONBUILD ${1}"
  }
}
